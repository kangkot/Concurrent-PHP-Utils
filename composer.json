{
    "name": "prolic/concurrent-php-utils",
    "type": "library",
    "description": "port of java.util.concurrent for php based on pthreads-extension",
    "keywords": ["pthreads" , "php", "concurrency", "multithreading"],
    "homepage": "https://github.com/prolic/ConcurrentPhpUtils",
    "license": "MIT",
    "authors": [
        {
            "name": "Sascha-Oliver Prolic",
            "email": "saschaprolic@googlemail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-uuid": ">=1.0.3",
        "ext-pthreads": "*",
        "marc-mabe/php-enum": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7",
        "phpmd/phpmd": "1.5.*"
    },
    "autoload": {
        "psr-0": {
            "ConcurrentPhpUtils\\": "src"
        }
    }
}
